{
	"name": "seltocss",
	"displayName": "Quick sass to css",
	"publisher": "plutopix",
	"description": "Write sass into css file and tranform selection into css",
	"icon": "images/icon.png",
	"repository": "https://github.com/PaulCrisan/seltocss",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:seltocss.selectedToCss"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "seltocss.selectedToCss",
				"title": "Transform selected sass to css"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/node-sass": "^4.11.1",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"sass": "^1.29.0"
	}
}
